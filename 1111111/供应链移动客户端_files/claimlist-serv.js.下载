/**
 *
 *claimlistService
 *   属性包括：
 *getSkpaymentRece: 获取展示列表
 *getSkPaymentInfo  详情
 */
angular.module('myApp').service('claimlistService',["$es",function($es){
	 
    this.getSkpaymentRece = function(filter,page,searchVal){
    	var param = {};
    	var limit = 5;

    	param.page = page;
    	param.start = page*limit;
    	param.limit = limit;
    	param.searchKey = searchVal;
        for(var key in filter){
                param[key] = filter[key];
        }
    	var promise = $es.ajax("skpaymentReceBean.getSkpaymentRece",param);
    	return promise;
    };

    
    this.getSkPaymentReceInfo = function(receiptNo){
       	var params=　{"receiptNo":receiptNo}; 
	   	var promise = $es.ajax('skpaymentReceBean.getSkPaymentReceInfo',params);
	   	return promise;
    };
}]);

